  "dependencies": {
    "compressing": "1.6.2",
    "shelljs": "0.8.5",
    "octokit": "2.0.7",
    "typed-rest-client": "1.8.9",
    "pbxproj-dom": "1.2.0"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "assert": "^2.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
